  "description": "[![Build Status](https://travis-ci.org/anishkny/node-express-realworld-example-app.svg?branch=master)](https://travis-ci.org/anishkny/node-express-realworld-example-app)",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "start:watch": "nodemon",
    "prestart:prod": "tsc",
    "start:prod": "node dist/main.js",
    "build": "tsc",
    "test": "jest --config=jest.json",
    "test:watch": "jest --watch --config=jest.json",
    "test:coverage": "jest --config=jest.json --coverage --coverageDirectory=coverage",
    "test:integration": "jest --config=./test/jest-test.json",
    "test:acceptance": "jest --config=./test/jest-acceptance.json --runInBand",
    "test:acceptance:report": "npm run test:acceptance && node scripts/generate-acceptance-report.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lujakob/nestjs-realworld-example-app.git"
