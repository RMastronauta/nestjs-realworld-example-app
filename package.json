    "crypto-js": "^4.0.0",
    "jsonwebtoken": "^8.5.1",
    "mysql2": "^3.14.1",
    "passport-jwt": "^4.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.5",
    "slug": "^1.1.0",
    "swagger-ui-express": "^4.1.4",
    "typeorm": "^0.2.41",
    "typescript": "^3.8.3"
  },
  "devDependencies": {
    "@types/jest": "^25.2.1",
    "@types/node": "^13.13.4",
    "atob": ">=2.1.0",
    "deep-extend": ">=0.5.1",
    "extend": ">=3.0.2",
    "jest": "^25.5.4",
